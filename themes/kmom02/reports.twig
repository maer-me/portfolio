{% include "incl/header.twig" %}

<body>
	{% include "incl/nav.twig" %}

	<div class="main tech-landing" role="main">
		{{ content }}
		{% for page in pages("analysis/") %}
			{% if page.title != "Analysis" %}
				<a href="{{page.url}}">
					<div class="tech-box">
						<div class="upper">
							{{ page.title }}
						</div>
						<div class="lower">
							{{ page.title}}
						</div>
					</div>
				</a>
			{% endif %}
		{% endfor %}
	</div>

	{% include "incl/footer.twig" %}
</body></html
